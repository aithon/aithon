# Properties file for Aithon plugin
# $Id$

# jEdit only needs to load the plugin the first time the user accesses it
# the presence of this property also tells jEdit the plugin is using the new API
plugin.AithonPlugin.activate=defer

# These two properties are required for all plugins
plugin.AithonPlugin.name=Aithon
plugin.AithonPlugin.author=Aithon Development Team

# Even if you don't store additional files, this is a good idea to set:
plugin.AithonPlugin.usePluginHome=true

# version number == jEdit version number
plugin.AithonPlugin.version=1.0

# online help
plugin.AithonPlugin.docs=index.html

# List dependencies of plugins with .depend.N property suffix:
plugin.AithonPlugin.depend.0=jdk 1.6
# See jEdit.getBuild() to understand version numbering scheme.
plugin.AithonPlugin.depend.1=jedit 05.00.01.00

plugin.AithonPlugin.description=This plugin provides support for the Aithon board.
# plugin menu
plugin.AithonPlugin.menu=aithon \
	- \
	aithon.choose-file \
	aithon.save-file \
	aithon.copy-to-buffer

# action labels for actions supplied by dockables.xml
aithon.label=Aithon

# action labels for actions supplied by actions.xml
aithon.choose-file.label=Choose notepad file
aithon.save-file.label=Save notepad file
aithon.copy-to-buffer.label=Copy notepad to buffer

# plugin option pane
plugin.AithonPlugin.option-pane=aithon

# Option pane activation BeanShell snippet
options.aithon.code=new AithonOptionPane();

# Option pane labels
options.aithon.label=Aithon
options.aithon.choose-file=Choose
options.aithon.choose-file.title=Choose a notepad file
options.aithon.choose-font=Font:
options.aithon.show-filepath.title=Display notepad file path
options.aithon.gcc-label=Compiler Location:
options.aithon.library-label=Library Location:
options.aithon.prog-label=Programmer Location:

# window title
aithon.title=Aithon

# window toolbar buttons
aithon.choose-file.icon=Open.png
aithon.save-file.icon=Save.png
aithon.copy-to-buffer.icon=CopyToBuffer.png

# default settings
options.aithon.show-filepath=true
options.aithon.font=Monospaced
options.aithon.fontstyle=0
options.aithon.fontsize=14

# Setting not defined but supplied for completeness
options.aithon.gcc-filepath=
options.aithon.library-filepath=
options.aithon.programmer-filepath=
